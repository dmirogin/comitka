{
    "require": {
        "yiisoft/yii2": "2.*",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "2.*",
        "kalyabin/yii2-git-view": "*",
        "kalyabin/yii2-hg-view": "*",
        "DmitryBaranovskiy/raphael": "^2.0",
        "yiisoft/yii2-imagine": "~2.0.0"
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    },
    "minimum-stability": "dev",
    "require-dev": {
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-debug": "*",
        "phpunit/phpunit": "*",
        "codeception/codeception": "*"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "codeception/codeception",
                "version": "dev-master",
                "source": {
                    "url": "https://github.com/kalyabin/Codeception",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "isagalaev/highlight.js",
                "version": "9.2.0",
                "source": {
                    "url": "https://github.com/isagalaev/highlight.js",
                    "type": "git",
                    "reference": "9.2.0"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "DmitryBaranovskiy/raphael",
                "version": "2.0",
                "source": {
                    "url": "https://github.com/DmitryBaranovskiy/raphael.git",
                    "type": "git",
                    "reference": "2.0"
                }
            }
        }
    ]
}
